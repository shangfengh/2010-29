#include<ctime>
using namespace std;
long long i,j,k,n,m,il[5][5][4],ww,ll,rr;
//1=A 0=a
inline bool bl(){
	ww=(rand()+rand()+rand())%2;
//	if(ww)++ll;else ++rr;
	return ww;
}
int main(){
	il[1][1][0]=il[1][2][0]=il[1][3][0]=il[1][4][0]=1;
    scanf("%lld",&n);
	srand(time(NULL));
